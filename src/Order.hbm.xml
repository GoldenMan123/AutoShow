<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
		
<hibernate-mapping>	
	<class name="logic.Order" table="Orders">
		<id column="number" name="number" type="java.lang.Long">
			<generator class="increment"/>
		</id>
		
		<property column="date" name="date" type="java.text.SimpleDateFormat"/>
		<property column="test_drive" name="testDrive" type="java.lang.Boolean"/>
		<property column="status_id" name="status" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>